{
    "Serilog": {
        "Using": [ "SyncSoft.App.Serilog" ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            {
                "Name": "Trace",
                "Args": { "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3} <{SourceContext}> {Message}{NewLine}{Exception}" }
            }
        ]
    }
}